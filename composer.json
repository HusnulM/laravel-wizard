{
    "name": "ycs77/laravel-wizard",
    "description": "A web setup wizard for your Laravel application.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Yang",
            "email": "yangchenshin77@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "wizard",
        "step",
        "steps",
        "component",
        "plugin"
    ],
    "homepage": "https://github.com/ycs77/laravel-wizard",
    "require": {
        "php": "^7.0",
        "illuminate/console": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5",
        "illuminate/view": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Ycs77\\LaravelWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ycs77\\LaravelWizard\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ycs77\\LaravelWizard\\WizardServiceProvider"
            ],
            "aliases": {
                "Wizard": "Ycs77\\LaravelWizard\\Facades\\Wizard"
            }
        }
    },
    "minimum-stability": "stable"
}
